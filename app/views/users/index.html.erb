<% provide(:title, "All users") %>
<h2>All users are listed below</h2>

<%= will_paginate %>

<ul class="users">
	<% @users.each do |user| %>
		<li><%= link_to user.name, user_path(user) %>
			<%= link_to "delete", user, method: :delete, 
										data: { confirm: "Are you sure?" } %></li>
	<% end %>
</ul>

<%= will_paginate %>